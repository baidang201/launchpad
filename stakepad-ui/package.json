{
  "name": "stakepad-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn build:pbjs && yarn build:pbts && yarn typegen:defs && yarn typegen:meta && next build",
    "build:pbjs": "pbjs -t static-module -o ./libs/apis/proto/api.js ../proto/message.proto",
    "build:pbjs-json": "pbjs -t json -o ./libs/apis/proto/api.json ../proto/message.proto",
    "build:pbts": "pbjs -t static-module ../proto/message.proto | pbts -o ./libs/apis/proto/api.d.ts -",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "next start",
    "typegen:defs": "ts-node --skip-project node_modules/@polkadot/typegen/scripts/polkadot-types-from-defs.cjs --package '.' --input ./libs/polkadot/interfaces",
    "typegen:meta": "ts-node --skip-project node_modules/@polkadot/typegen/scripts/polkadot-types-from-chain.cjs --package '.' --endpoint wss://poc4.phala.network/ws --output ./libs/polkadot/interfaces"
  },
  "dependencies": {
    "@polkadot/api": "^4.4.1",
    "@polkadot/extension-dapp": "^0.37.2",
    "@polkadot/util": "^6.0.5",
    "@polkadot/util-crypto": "^6.0.5",
    "@types/uuid": "^8.3.0",
    "baseui": "^9.112.0",
    "bunyan": "^1.8.15",
    "echarts": "^5.0.2",
    "echarts-for-react": "^3.0.1",
    "next": "10.0.8",
    "node-fetch": "^2.6.1",
    "protobufjs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.13.4",
    "styletron-engine-atomic": "^1.4.7",
    "styletron-react": "^6.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@phala/typedefs": "^0.1.0",
    "@polkadot/typegen": "^4.7.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-baseui": "^9.112.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
